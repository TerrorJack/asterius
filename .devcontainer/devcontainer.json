{
  "name": "asterius",
  "dockerFile": "Dockerfile",
  "appPort": [3000],
  "remoteUser": "asterius",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "postCreateCommand": ".devcontainer/build.sh",
  "extensions": [
    "justusadam.language-haskell",
    "hoovercj.haskell-linter",
    "maxgabriel.brittany",
    "sjurmillidahl.ormolu-vscode",
    "esbenp.prettier-vscode",
    "stkb.rewrap"
  ]
}
